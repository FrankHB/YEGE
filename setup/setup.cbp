<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="setup" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Option extended_obj_names="1" />
		<Build>
			<Target title="debug">
				<Option output="debug/setup" prefix_auto="1" extension_auto="1" />
				<Option object_output="debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option use_console_runner="0" />
				<Compiler>
					<Add option="-g" />
					<Add option="-Wa,-mbig-obj" />
					<Add option="-D_GLIBCXX_DEBUG_PEDANTIC" />
				</Compiler>
				<Linker>
					<Add library="..\EGE\debug\libEGE.a" />
					<Add library="z" />
					<Add library="png16" />
					<Add library="gdi32" />
					<Add library="msimg32" />
					<Add library="ole32" />
					<Add library="oleaut32" />
					<Add library="winmm" />
					<Add library="uuid" />
					<Add library="gdiplus" />
				</Linker>
			</Target>
			<Target title="release">
				<Option output="release/setup" prefix_auto="1" extension_auto="1" />
				<Option object_output="release/" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-fomit-frame-pointer" />
					<Add option="-fexpensive-optimizations" />
					<Add option="-O3" />
					<Add option="-fno-enforce-eh-specs" />
					<Add option="-flto=jobserver" />
					<Add option="-DNDEBUG" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="-flto=jobserver" />
					<Add option="-Wl,-allow-multiple-definition" />
					<Add library="..\EGE\release\libEGE.a" />
					<Add library="z" />
					<Add library="png16" />
					<Add library="gdi32" />
					<Add library="msimg32" />
					<Add library="ole32" />
					<Add library="oleaut32" />
					<Add library="winmm" />
					<Add library="uuid" />
					<Add library="gdiplus" />
				</Linker>
			</Target>
			<Target title="debug_DLL">
				<Option output="debug/setup" prefix_auto="1" extension_auto="1" />
				<Option object_output="debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option use_console_runner="0" />
				<Compiler>
					<Add option="-g" />
					<Add option="-Wa,-mbig-obj" />
					<Add option="-D_GLIBCXX_DEBUG_PEDANTIC" />
				</Compiler>
				<Linker>
					<Add library="..\EGE\debug\EGE.dll" />
				</Linker>
				<ExtraCommands>
					<Add before='CMD /C DEL &quot;debug\EGE.dll&quot;' />
					<Add before='CMD /C MKLINK &quot;debug\EGE.dll&quot; &quot;..\..\EGE\debug\EGE.dll&quot; || MKLINK /H &quot;debug\EGE.dll&quot; &quot;..\EGE\debug\EGE.dll&quot; || ECHO F | XCOPY /D /Y &quot;..\EGE\debug\EGE.dll&quot; &quot;debug\EGE.dll&quot;' />
				</ExtraCommands>
			</Target>
			<Target title="release_DLL">
				<Option output="release/setup" prefix_auto="1" extension_auto="1" />
				<Option object_output="release/" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-fomit-frame-pointer" />
					<Add option="-fexpensive-optimizations" />
					<Add option="-O3" />
					<Add option="-fno-enforce-eh-specs" />
					<Add option="-flto=jobserver" />
					<Add option="-DNDEBUG" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="-flto=jobserver" />
					<Add option="-Wl,-allow-multiple-definition" />
					<Add library="..\EGE\release\EGE.dll" />
				</Linker>
				<ExtraCommands>
					<Add before='CMD /C DEL &quot;release\EGE.dll&quot;' />
					<Add before='CMD /C MKLINK &quot;release\EGE.dll&quot; &quot;..\..\EGE\release\EGE.dll&quot; || MKLINK /H &quot;release\EGE.dll&quot; &quot;..\EGE\release\EGE.dll&quot; || ECHO F | XCOPY /D /Y &quot;..\EGE\release\EGE.dll&quot; &quot;release\EGE.dll&quot;' />
				</ExtraCommands>
			</Target>
		</Build>
		<Compiler>
			<Add option="-pedantic" />
			<Add option="-Wextra" />
			<Add option="-Wall" />
			<Add option="-fnew-inheriting-ctors" />
			<Add option="-fno-strong-eval-order" />
			<Add option="-std=c++11" />
			<Add option="-mthreads" />
			<Add option="-U__GXX_TYPEINFO_EQUALITY_INLINE" />
			<Add option="-U__GXX_MERGED_TYPEINFO_NAMES" />
			<Add option="-D__GXX_TYPEINFO_EQUALITY_INLINE=1" />
			<Add option="-D__GXX_MERGED_TYPEINFO_NAMES=0" />
			<Add directory="../EGE/include" />
		</Compiler>
		<Linker>
			<Add option="-Wl,--gc-sections" />
			<Add option="-pipe" />
		</Linker>
		<Unit filename="setup.cpp" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
